import java.util.ArrayList;
import java.util.List;

public class QueryManager {

	private static QueryManager queryManager;
	StudentManager studentManager = StudentManager.getInstance();
	KlassManager klassManager = KlassManager.getInstance();
	
	public static QueryManager getInstance() {
		if (queryManager == null) {
			queryManager = new QueryManager();
		}
		return queryManager;
	}

	
	private List<String> findCommonStudentToKlass(List<String> klasses){
		List<String> commonStudent = new ArrayList<String>();
		List<Student> allStudent = studentManager.getAllStudent();
		return commonStudent;
		
	}
}
